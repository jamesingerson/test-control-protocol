{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Test Control Protocol",
	"patterns": [
		{
			"include": "#invalid"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#title"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#alpha-line"
		}
	],
	"repository": {
		"invalid": {
			"patterns": [
				{
					"name": "invalid.too-long.testcontrolprotocol",
					"match": "^.{80}.*\n"
				},
				{
					"name": "invalid.illegal-character.testcontrolprotocol",
					"match": "^.{6}[^\\s].*\n"
				}
			]
		},
		"comments": {
			"patterns": [{
				"name": "comment.testcontrolprotocol",
				"match": "^.{7}\\*.*\n"
			}]
		},
		"title": {
			"patterns": [{
				"name": "support.type.testcontrolprotocol",
				"match": "^.{7}T.*\n"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.testcontrolprotocol",
				"match": "\\b(GOTO|PRINT|MOVE)\\b"
			}]
		},
		"alpha-line": {
			"captures": {
				"1": {
					"name": "keyword.control.testcontrolprotocol"
				},
				"2": {
					"name": "storage.control.testcontrolprotocol"
				},
				"3": {
					"name": "string.control.testcontrolprotocol"
				}
			},
			"match": "(A)(\\s.{8})(\\s.*)",
			"name": "alpha-line.testcontrolprotocol"
		}
	},
	"scopeName": "source.testcontrolprotocol"
}